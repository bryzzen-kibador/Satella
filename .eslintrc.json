{
    "env": {
        "browser": true,
        "commonjs": true,
        "es2021": true
    },
    "extends": [
        "airbnb-base"
    ],
    "parserOptions": {
        "ecmaVersion": 12
    },
    "rules": {
      "getter-return": "off",
      "no-underscore-dangle": "off",
      "global-require": "off",
      "no-unused-vars": "off",
      "no-param-reassign": "off",
      "no-console": "off",
      "consistent-return": "off",
      "no-bitwise": "off",
      "no-shadow": "off",
      "no-eval": "off",
      "no-unused-expressions": "off",
      "no-mixed-operators": "off",
      "no-restricted-syntax": "off",
      "quote-props": "off",
      "import/no-dynamic-require": "off",
      "object-curly-newline": "off",
      "eqeqeq": "off",
      "class-methods-use-this": "off"
    }
}
